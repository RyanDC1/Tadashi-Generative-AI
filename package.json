{
  "name": "gen_ai",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm i -g pnpm && pnpm run build-ui && pnpm run build-server",
    "start": "cd server && pnpm start",
    "build-win": "pnpm i && pnpm run build && @powershell Copy-Item dist -Recurse ./server",
    "build-ui": "pnpm i && vite build && cp -r dist ./server",
    "build-server": "cd server && pnpm i",
    "build-local": "pnpm run build-win && pnpm run build-server",
    "start-server": "cd server && pnpm run start"
  },
  "dependencies": {
    "@ant-design/icons": "^5.2.6",
    "@reduxjs/toolkit": "^2.0.1",
    "@uiw/react-markdown-preview": "^5.0.4",
    "antd": "^5.11.5",
    "axios": "^1.6.2",
    "dayjs": "^1.11.10",
    "hybrid-crypto-js": "^0.2.4",
    "lodash": "^4.17.21",
    "node-forge": "^1.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^9.0.4",
    "react-reveal": "^1.2.2",
    "redux": "^5.0.0",
    "typewriter-effect": "^2.21.0"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.202",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@types/react-redux": "^7.1.33",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitejs/plugin-react": "^4.2.0",
    "eslint": "^8.53.0",
